{
  "root": true,
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "node": true,
    "browser": true
  },
  "extends": "airbnb",
  "rules": {
    "linebreak-style": 0,
    "quote-props": "off",
    "no-console": "off",
    "no-irregular-whitespace": "error",
    "no-unused-vars": "error",
    "no-multiple-empty-lines": "error"
  }
}